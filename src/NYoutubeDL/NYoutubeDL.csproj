<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Version>0.11.2</Version>
    <Authors>Robert Ankeny</Authors>
    <Company />
    <Product>NYoutubeDLP</Product>
    <Description>A simple youtube-dlp library for C#.</Description>
    <Copyright>Copyright Brian Allred 2021</Copyright>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageProjectUrl>https://github.com/ankenyr/NYoutubeDLP</PackageProjectUrl>
    <PackageTags>youtube-dlp</PackageTags>
    <NeutralLanguage></NeutralLanguage>
    <AssemblyVersion>0.11.2.0</AssemblyVersion>
    <FileVersion>0.11.2.0</FileVersion>
    <PackageId>NYoutubeDLP</PackageId>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="10.0.3" />
  </ItemGroup>
</Project>
